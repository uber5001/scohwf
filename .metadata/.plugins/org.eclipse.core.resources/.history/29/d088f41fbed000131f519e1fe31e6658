
public class UserInfo {
	private User self;
	private Track[] tracks;
	
	UserInfo(int amount){
		setSelf(new User());
		setTracks(new Track[amount]);
	}

	public User getSelf() {
		return self;
	}

	public void setSelf(User self) {
		this.self = self;
	}

	public Track[] getTracks() {
		return tracks;
	}

	public void setTracks(Track[] tracks) {
		this.tracks = tracks;
	}
	
	public void addTrack(Track track, int i) {
		this.tracks[i] = track;
	}
}
